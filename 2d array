import java.util.*;
public class Array{
   public static void main(String[] args)
   {
      //int [][] grades= new int[3][];//5 row and col 
      int [][] grades={{1, 5 ,3},{1,3,5,9},{4,5,6}};
      
      for(int row=0; row<grades.length; row++)// how you access the row
      {
         for(int col=0; col<grades[0].length; col++)// access the col
         {
            System.out.println(grades[col][row]);//Array.deepToString(grades)
         }
      }
      
   }
}
